<robot name = "quadrotor">
<!--For Battery Box-->
	<link name = "body_link">
		<visual name = "">
        	<origin xyz = "0 0 0.06" rpy = "0 0 0"/>
		<geometry>
			<box size="0.3 0.3 0.12"/>
		</geometry>
		<material name = "colour1">
			<color rgba="0 0.5 1, 1"/>
		</material>
		</visual>
	</link>
<!--For Arms of the drone-->
	<link name = "arm1_link">
		<visual name = "" >
		<origin xyz = "0.15 0 0" />
		<geometry>
			<box size="0.35 0.1 0.10"/>
		</geometry>
		<material name = "colour2">
			<color rgba="1 0.5 0 1"/>
		</material> 		
		</visual>

	</link>
	<link name = "arm2_link">
 		<visual name = "" >
 		<origin xyz = "-0.15 0 0" />
 		<geometry>
 			<box size="0.35 0.1 0.10"/>
 		</geometry>
 		<material name = "colour2">
 			<color rgba="1 0.5 0 1"/>
 		</material>
 		</visual>

 	</link>
	<link name = "arm3_link">
 		<visual name = "" >
 		<origin xyz = "0 0.15 0" />
 		<geometry>
 			<box size="0.1 0.35 0.10"/>
 		</geometry>
 		<material name = "colour2">
 			<color rgba="1 0.5 0 1"/>
 		</material>
 		</visual>

 	</link>
	<link name = "arm4_link">
 		<visual name = "" >
 		<origin xyz = "0 -0.15 0" />
 		<geometry>
 			<box size="0.1 0.35 0.10"/>
 		</geometry>
 		<material name = "colour2">
 			<color rgba="1 0.5 0 1"/>
 		</material>
 		</visual>

 	</link>
<!--For Motors-->
	<link name = "motor1_link">
 		<visual name = "" >
 		<origin xyz = "0 0 0" />
 		<geometry>
 			<cylinder radius = "0.03" length = "0.04" />
 		</geometry>
 		<material name = "colour3">
 			<color rgba="1 0 1 1"/>
 		</material>
 		</visual>

 	</link>
	<link name = "motor2_link">
 		<visual name = "" >
 		<origin xyz = "0 0 0" />
 		<geometry>
 			<cylinder radius = "0.03" length = "0.04" />
 		</geometry>
 		<material name = "colour3">
 			<color rgba="1 0 1 1"/>
 		</material>
 		</visual>

 	</link>
	<link name = "motor3_link">
 		<visual name = "" >
 		<origin xyz = "0 0 0" />
 		<geometry>
 			<cylinder radius = "0.03" length = "0.04" />
 		</geometry>
 		<material name = "colour3">
 			<color rgba="1 0 1 1"/>
 		</material>
 		</visual>

 	</link>
	<link name = "motor4_link">
 		<visual name = "" >
 		<origin xyz = "0 0 0" />
 		<geometry>
 			<cylinder radius = "0.03" length = "0.04" />
 		</geometry>
 		<material name = "colour3">
 			<color rgba="1 0 1 1"/>
 		</material>
 		</visual>

 	</link>
<!--For Propellers/Fans of the drone-->
	<link name = "fan1_link">
 		<visual name = "" >
 		<origin xyz = "0 0 0" />
 		<geometry>
 			<box size = "0.2 0.2 0.002" />
 		</geometry>
 		<material name = "colour4">
 			<color rgba="0 0 1 1"/>
 		</material>
 		</visual>

 	</link>
	<link name = "fan2_link">
 		<visual name = "" >
 		<origin xyz = "0 0 0" />
 		<geometry>
 			<box size = "0.2 0.2 0.002" />
 		</geometry>
 		<material name = "colour4">
 			<color rgba="0 0 1 1"/>
 		</material>
 		</visual>

 	</link>
	<link name = "fan3_link">
 		<visual name = "" >
 		<origin xyz = "0 0 0" />
 		<geometry>
 			<box size = "0.2 0.2 0.002" />
 		</geometry>
 		<material name = "colour4">
 			<color rgba="0 0 1 1"/>
 		</material>
 		</visual>

 	</link>
	<link name = "fan4_link">
 		<visual name = "" >
 		<origin xyz = "0 0 0" />
 		<geometry>
 			<box size = "0.2 0.2 0.002" />
 		</geometry>
 		<material name = "colour4">
 			<color rgba="0 0 1 1"/>
 		</material>
 		</visual>

 	</link>


<!--For joints between arms and battery box-->
        <joint name = "arm1_joint" type= "fixed">
		<parent link = "body_link" />
		<child link = "arm1_link" />
		<origin xyz= "0.15 0 0.06" />
	</joint>
        <joint name = "arm2_joint" type= "fixed">
		<parent link = "body_link" />
		<child link = "arm2_link" />
		<origin xyz= "-0.15 0 0.06" />
	</joint>
        <joint name = "arm3_joint" type= "fixed">
		<parent link = "body_link" />
		<child link = "arm3_link" />
		<origin xyz= "0 0.15 0.06" />
	</joint>
        <joint name = "arm4_joint" type= "fixed">
		<parent link = "body_link" />
		<child link = "arm4_link" />
	<origin xyz= "0 -0.15 0.06" />
	</joint>
<!--For Joints between Motor and Arms-->
        <joint name = "motor1_joint" type= "fixed">
		<parent link = "arm1_link" />
		<child link = "motor1_link" />
		<origin xyz= "0.25 0 0.07" />
	</joint>
	<joint name = "motor2_joint" type= "fixed">
		<parent link = "arm2_link" />
		<child link = "motor2_link" />
		<origin xyz= "-0.25 0 0.07" />
	</joint>
	<joint name = "motor3_joint" type= "fixed">
		<parent link = "arm3_link" />
		<child link = "motor3_link" />
		<origin xyz= "0 0.25 0.07" />
	</joint>
	<joint name = "motor4_joint" type= "fixed">
		<parent link = "arm4_link" />
		<child link = "motor4_link" />
		<origin xyz= "0 -0.25 0.07" />
	</joint>
<!--For joints between motor and propellers-->
	<joint name = "fan1_joint" type= "continuous">
		<parent link = "motor1_link" />
		<child link = "fan1_link" />
		<origin xyz= "0 0 0.021" />
		<axis xyz = "0 0 1" />
	</joint>
	<joint name = "fan2_joint" type= "continuous">
		<parent link = "motor2_link" />
		<child link = "fan2_link" />
		<origin xyz= "0 0 0.021" />
		<axis xyz = "0 0 1" />
	</joint>
	<joint name = "fan3_joint" type= "continuous">
		<parent link = "motor3_link" />
		<child link = "fan3_link" />
		<origin xyz= "0 0 0.021" />
		<axis xyz = "0 0 1" />
	</joint>
	<joint name = "fan4_joint" type= "continuous">
		<parent link = "motor4_link" />
		<child link = "fan4_link" />
		<origin xyz= "0 0 0.021" />
		<axis xyz = "0 0 1" />
	</joint>



</robot>